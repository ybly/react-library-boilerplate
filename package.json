{
    "name": "react-library-boilerplate",
    "version": "1.0.0",
    "description": "React library boilerplate",
    "main": "dist/bundle.cjs.js",
    "module": "dist/bundle.esm.js",
    "sideEffects": false,
    "dependencies": {},
    "devDependencies": {
        "@babel/core": "^7.11.1",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-react": "^7.10.4",
        "@rollup/plugin-babel": "^5.1.0",
        "eslint": "^7.6.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.20.5",
        "eslint-plugin-standard": "^4.0.1",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.11",
        "prettier": "2.0.5",
        "rimraf": "^3.0.2",
        "rollup": "^2.23.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^6.1.0"
    },
    "peerDependencies": {
        "react": ">=15.6.1",
        "react-dom": ">=15.6.1",
        "lodash": ">=4.17.4",
        "moment": ">=2.18.1"
    },
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "lint": "eslint \"**/*.{js,jsx}\"",
        "lint:fix": "prettier \"**/*.{js,jsx}\" --write",
        "prebuild": "rimraf dist",
        "build": "rollup -c"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "npm run lint:fix",
            "git add"
        ],
        "*.jsx": [
            "npm run lint:fix",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ybly/react-library-boilerplate.git"
    },
    "keywords": [
        "react"
    ],
    "author": "Bo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ybly/react-library-boilerplate/issues"
    },
    "homepage": "https://github.com/ybly/react-library-boilerplate#readme"
}
